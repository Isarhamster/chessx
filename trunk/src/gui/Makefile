$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: gui.pro  /usr/qt/3/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile gui.pro
qmake: 
	@$(QMAKE) -o Makefile gui.pro

dist: 
	@mkdir -p .tmp/gui && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/gui/ && ( cd `dirname .tmp/gui` && $(TAR) gui.tar gui && $(GZIP) gui.tar ) && $(MOVE) `dirname .tmp/gui`/gui.tar.gz . && $(DEL_FILE) -r .tmp/gui

