MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-src-database \
		sub-src-gui \
		sub-src

first: all

all: Makefile $(SUBTARGETS)

src/database/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "src/database" || $(MKDIR) "src/database"
	cd src/database && $(QMAKE) database.pro -o $(MAKEFILE)
sub-src-database: src/database/$(MAKEFILE) FORCE
	cd src/database && $(MAKE) -f $(MAKEFILE)

src/gui/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "src/gui" || $(MKDIR) "src/gui"
	cd src/gui && $(QMAKE) gui.pro -o $(MAKEFILE)
sub-src-gui: src/gui/$(MAKEFILE) FORCE
	cd src/gui && $(MAKE) -f $(MAKEFILE)

src/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "src" || $(MKDIR) "src"
	cd src && $(QMAKE) src.pro -o $(MAKEFILE)
sub-src: src/$(MAKEFILE) FORCE
	cd src && $(MAKE) -f $(MAKEFILE)

Makefile: chessdatabase.pro  /usr/qt/3/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile chessdatabase.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile chessdatabase.pro

